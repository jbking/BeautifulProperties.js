{
  "name": "BeautifulProperties",
  "version": "0.1.9",
  "repository": {
    "type": "git",
    "url": "https://github.com/monjudoh/BeautifulProperties.js.git"
  },
  "private": true,
  "devDependencies": {
    "jsdoc": "~3.3.0-alpha13",
    "underscore": "~1.6",
    "amdbuilder": "=0.0.7"
  },
  "engine": {
    "node": ">= 0.10",
    "npm": ">= 2.1"
  },
  "scripts": {
    "build" : "node build.js",
    "docs" : "jsdoc -r src/ -d docs/",
    "docs:clean" : "rm -rf docs",
    "docs-inner" : "jsdoc -r -p src/ -d docs-inner/",
    "docs-inner:clean" : "rm -rf docs-inner/"
  }
}
